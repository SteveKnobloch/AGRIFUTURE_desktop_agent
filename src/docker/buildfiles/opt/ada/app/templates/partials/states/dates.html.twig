            {% include 'partials/states/dates/dateAndTime.html.twig' with {'date': datePaused, small: false} %}
        </dd>
    </dl>
{% endif %}

{% if dateCompleted is defined %}
    <dl class="d-table-row d-table">
        <dt class="d-table-cell strong">
            {{ 'Completed' | trans }}:
        </dt>
        <dd class="d-table-cell px-3">
            {% include 'partials/states/dates/dateAndTime.html.twig' with {'date': dateCompleted, small: false} %}
        </dd>
    </dl>
{% endif %}

{% if dateShutdown is defined %}
    <dl class="d-table-row d-table">
        <dt class="d-table-cell strong">
            {{ 'Analysis ends automatically on' | trans }}:
        </dt>
        <dd class="d-table-cell px-3">
            {% include 'partials/states/dates/dateAndTime.html.twig' with {'date': dateShutdown, small: false} %}
        </dd>
