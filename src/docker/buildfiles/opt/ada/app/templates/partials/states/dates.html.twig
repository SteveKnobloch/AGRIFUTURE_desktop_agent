{% if dateStart is defined %}
    <dl class="d-table-row d-table">
        <dt class="d-table-cell strong">
            {{ 'Started' | trans }}:
        </dt>
        <dd class="d-table-cell px-3">
            {% include 'partials/states/dates/dateAndTime.html.twig' with {'date': dateStart, small: false} %}
        </dd>
    </dl>
{% endif %}

{% if dateCrashed is defined %}
    <dl class="d-table-row d-table">
        <dt class="d-table-cell strong">
            {{ 'Crashed' | trans }}:
        </dt>
        <dd class="d-table-cell px-3">
            {% include 'partials/states/dates/dateAndTime.html.twig' with {'date': dateCrashed, small: false} %}
        </dd>
    </dl>
{% endif %}

{% if datePaused is defined %}
